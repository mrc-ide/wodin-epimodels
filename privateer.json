{
    "servers": [
        {
            "name": "annex2",
            "hostname": "annex2.montagu.dide.ic.ac.uk",
            "port": 10023,
            "container": "privateer_server_wodin_epimodels"
        }
    ],
    "clients": [
        {
            "name": "epimodels",
            "backup": ["redis-data"],
            "schedule": {
                "port": 9090,
                "jobs": {
                    "server": "annex2",
                    "volume": "redis-data",
                    "schedule": "@hourly"
                }
            }
        }
    ],
    "volumes": [
        {
            "name": "redis-data"
        }
    ],
    "vault": {
        "url": "https://vault.dide.ic.ac.uk:8200",
        "prefix": "/secret/wodin/privateer/epimodels"
    }
}
