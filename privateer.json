{
    "servers": [
        {
            "name": "annex2",
            "hostname": "annex2.montagu.dide.ic.ac.uk",
            "port": 10023,
            "key_volume": "privateer_wodin_epimodels_keys",
            "data_volume": "privateer_wodin_epimodels_data",
            "container": "privateer_wodin_epimodels_server"
        }
    ],
    "clients": [
        {
            "name": "epimodels",
            "backup": ["redis-data"],
            "schedule": {
                "port": 9090,
                "jobs": [
                    {
                        "server": "annex2",
                        "volume": "redis-data",
                        "schedule": "@hourly"
                    }
                ]
            }
        }
    ],
    "volumes": [
        {
            "name": "redis-data"
        }
    ],
    "vault": {
        "url": "https://vault.dide.ic.ac.uk:8200",
        "prefix": "/secret/wodin/privateer/epimodels"
    }
}
